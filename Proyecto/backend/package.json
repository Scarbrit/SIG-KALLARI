{
  "type": "module",
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.js",
    "create-schemas": "node -r dotenv/config src/utils/createSchemas.js",
    "setup-db": "node -r dotenv/config scripts/setupDatabase.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:create": "sequelize-cli migration:generate --name",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status",
    "seed": "sequelize-cli db:seed:all",
    "seed:create": "sequelize-cli seed:generate --name",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:undo:last": "sequelize-cli db:seed:undo",
    "db:init": "npm run create-schemas",
    "db:import-locations": "node src/scripts/import-locations.mjs",
    "db:setup": "npm run create-schemas && npm run migrate && npm run db:import-locations && npm run seed",
    "db:refresh": "npm run migrate:undo:all && npm run migrate && npm run db:import-locations && npm run seed",
    "db:reset": "npm run seed:undo && npm run migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "form-data": "^4.0.5",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-forge": "^1.3.3",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "read-excel-file": "^6.0.1",
    "sequelize": "^6.37.7",
    "soap": "^1.6.1",
    "xmlbuilder2": "^4.0.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  }
}